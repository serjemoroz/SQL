№3
SELECT model,ram,screen
FROM laptop
WHERE price >1000

№4
SELECT *
FROM printer
WHERE color='y'

№5
SELECT model,speed,hd
FROM PC
WHERE (cd='12x' OR cd='24x') AND price<600

№6
SELECT DISTINCT p.maker, l.speed
FROM laptop l
JOIN product p ON p.model = l.model
WHERE l.hd >= 10



№8
SELECT DISTINCT maker
FROM product
WHERE type='pc'
except
SELECT distinct product.maker
FROM product
WHERE type='laptop'


